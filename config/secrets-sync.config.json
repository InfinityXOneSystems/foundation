{
  "github": {
    "organization": "InfinityXOneSystems",
    "token_env_var": "GITHUB_TOKEN"
  },
  "sync": {
    "enabled": true,
    "interval_hours": 6,
    "auto_start": true
  },
  "repos": {
    "exclude": [],
    "include_archived": false,
    "include_private": true
  },
  "secrets": {
    "encryption_key_length": 32,
    "auto_rotate_keys": false
  }
}
